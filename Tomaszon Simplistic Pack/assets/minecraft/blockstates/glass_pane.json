{
  "multipart": [
    {
      "when":{ "north": false, "south":false, "west":false, "east":false },
      "apply": { "model": "block/glass_pane_post" }
    },
    {
      "when": { "north": true },
      "apply": { "model": "block/glass_pane_side_nw" }
    },
    {
      "when": { "south": true },
      "apply": { "model": "block/glass_pane_side_se" }
    },
    {
      "when": { "west": true },
      "apply": { "model": "block/glass_pane_side_nw", "y": 270 }
    },
    {
      "when": { "east": true },
      "apply": { "model": "block/glass_pane_side_se", "y": 270}
    }
  ]
}